            getArrow(onc, yeni) {
                if (onc === '-' || yeni === '-') return 'â†’';
                const o = parseFloat(onc);
                const y = parseFloat(yeni);
                if (isNaN(o) || isNaN(y)) return 'â†’';
                if (y > o) return 'â†‘';
                if (y < o) return 'â†“';
                return 'â†’';
            },

            getChange(onc, yeni) {
                if (onc === '-' || yeni === '-') return 'Yeni';
                const o = parseFloat(onc);
                const y = parseFloat(yeni);
                if (isNaN(o) || isNaN(y)) return '-';
                const diff = (y - o).toFixed(2);
                return diff > 0 ? `+${diff}` : diff;
            },

            statusBadge(durum) {
                const colors = {
                    'N': 'bg-green-100 text-green-800',
                    'H': 'bg-red-100 text-red-800',
                    'L': 'bg-yellow-100 text-yellow-800',
                    '-': 'bg-gray-100 text-gray-800'
                };
                const labels = { 'N': 'Normal', 'H': 'YÃ¼ksek', 'L': 'DÃ¼ÅŸÃ¼k', '-': '-' };
                return `<span class="px-2 py-1 rounded-full text-xs font-semibold ${colors[durum]}">${labels[durum]}</span>`;
            },

