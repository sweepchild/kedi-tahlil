            navigate(page) {
                this.currentPage = page;
                this.render();
                window.scrollTo(0, 0);
            },

            render() {
                const pages = {
                    'home': this.renderHome(),
                    'luke': this.renderLuke(),
                    'yeleli': this.renderYeleli(),
                    'venus': this.renderVenus(),
                    'mirnav': this.renderMirnav()
                };
                document.getElementById('app').innerHTML = pages[this.currentPage];
            }
        };

        // BaÅŸlangÄ±Ã§
        App.render();
